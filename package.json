{
  "name": "nodetasksapi",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon --legacy-watch src/server.ts",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "migrate": "ts-node migrate.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --clearCache && jest --detectOpenHandles --testTimeout=20000 --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "npm cache clean --force && jest --clearCache && jest --detectOpenHandles --testTimeout=20000 --verbose --coverage",
    "test:clear": "npm cache clean --force && jest --clearCache && jest --detectOpenHandles --testTimeout=20000",
    "format": "prettier --write .",
    "prismainit": "npx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migrations/init/migration.sql && npx prisma migrate resolve --applied init",
    "prismareset": "rimraf prisma/migrations && node -e \"require('fs').mkdirSync('prisma/migrations', { recursive: true })\" && (npx prisma migrate reset --force || echo 'reset failed') && (npx prisma migrate dev --skip-generate || echo 'migrate dev failed') && (npx prisma generate || echo 'generate failed') && (npx prisma db seed || echo 'seed failed')"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^13.2.0",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "mailgen": "^2.0.29",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1",
    "qs": "^6.14.0",
    "rate-limit-redis": "^4.2.0",
    "resend": "^4.0.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.5.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/http-errors": "^2.0.4",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mailgen": "^2.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pdf-parse": "^1.1.5",
    "@types/qs": "^6.9.18",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.5.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.19.1",
    "wait-on": "^8.0.3"
  }
}
